; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "PokemonManager",
"language": "GDScript",
"path": "res://PokemonManager.gd"
} ]
_global_script_class_icons={
"PokemonManager": ""
}

[application]

config/name="Pokemon Spelling"
run/main_scene="res://interface/title_screen/TitleScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="pokespell"
config/icon="res://assets/icon.png"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.984314, 0.984314, 0.984314, 1 )
environment/default_environment="res://default_env.tres"
